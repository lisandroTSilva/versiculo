    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://127.0.0.1:8080",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "port": 9222,
            "runtimeArgs": [
                "--remote-debugging-port=9222"
